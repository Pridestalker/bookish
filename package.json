    "watch:js": "yarn run prod:js --watch",
    "watch:css": "yarn run prod:css --watch",
    "prod": "yarn prod:css && yarn prod:js",
    "prod:js": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod:css": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --env.mixfile=webpack.style.js --config=node_modules/laravel-mix/setup/webpack.config.js"
