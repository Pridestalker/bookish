  "scripts": {
    "dev": "yarn dev:css && yarn dev:js",
    "dev:js": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
